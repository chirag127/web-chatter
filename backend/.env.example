# Optional environment variables for the backend
# Note: The Gemini API key is provided by the user in each request

# Logging level (INFO, DEBUG, WARNING, ERROR)
LOG_LEVEL=INFO

# Gemini API model names
PRIMARY_GEMINI_MODEL=gemini-1.5-flash-latest
FALLBACK_GEMINI_MODEL=gemini-pro

# Token threshold for switching to fallback model
TOKEN_THRESHOLD_FOR_FALLBACK=200000
